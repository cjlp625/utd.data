sub.2005.9.ui.met <- subset(nchs.data, Year == 2005 & HHS.Region == 9 & Cause.of.Death == "Unintentional Injury" & Locality == "Metropolitan")
sub.2005.9.hd.met <- subset(nchs.data, Year == 2005 & HHS.Region == 9 & Cause.of.Death == "Heart Disease" & Locality == "Metropolitan")
sub.2005.9.clrd.met <- subset(nchs.data, Year == 2005 & HHS.Region == 9 & Cause.of.Death == "Chronic Lower Respiratory Disease" & Locality == "Metropolitan")
sub.2005.9.can.non <- subset(nchs.data, Year == 2005 & HHS.Region == 9 & Cause.of.Death == "Cancer" & Locality == "Nonmetropolitan")
sub.2005.9.str.non <- subset(nchs.data, Year == 2005 & HHS.Region == 9 & Cause.of.Death == "Stroke" & Locality == "Nonmetropolitan")
sub.2005.9.ui.non <- subset(nchs.data, Year == 2005 & HHS.Region == 9 & Cause.of.Death == "Unintentional Injury" & Locality == "Nonmetropolitan")
