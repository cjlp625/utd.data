# Utilizing extra tools
library(ggplot2)
library(UsingR)

# Loading data
nchs.data <- read.csv(file = 'nchs.csv', header = TRUE)

# Setting all blank values to 0
nchs.data[is.na(nchs.data)] <- 0


sub.2005.1.can.met <- subset(nchs.data, Year == 2005 & HHS.Region == 1 & Cause.of.Death == "Cancer" & Locality == "Metropolitan")
